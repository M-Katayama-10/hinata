<h1>トップページ</h1>
<% if user_signed_in? %>
   <%= link_to current_user.name, edit_user_path(current_user), class: "" %>
   <%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "" %>
   <%= link_to '投稿する', new_tweet_path, class: '' %>
<% else %>
   <%= link_to "ログイン", new_user_session_path %>
   <%= link_to "新規登録", new_user_registration_path %>
<% end %>


<% @tweets.each do |tweet| %>
  <div class="">
    <h3 class=''>
      <%= tweet.time %>
    </h3>

    <div class=''>
      <%= tweet.member.name %>
    </div class=''>

    <div class=''>
      <%= tweet.program %>
    </div class=''>

    <div class=''>
      <%= tweet.time %>
    </div class=''>

    <div class=''>
      <%= tweet.supplement %>
    </div class=''>
  </div>

<% end %>
